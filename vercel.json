{
    "builds": [{
      "src": "few/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.9"
      },
      "env": {
        "GDAL_CONFIG": "/usr/bin/gdal-config"
      }
    }],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "few/wsgi.py"
      }
    ]
  }
  